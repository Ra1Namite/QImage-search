{
  "manifest_version": 2,
  "name": "CropperImageSearch",
  "version": "0.2",
  "description": "google image search from screenshot",
  "author": "Yunik Maharjan <yunik.maharjan@gmail.com>",
  "homepage_url": "https://github.com/unique1o1/quicksave",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "144": "icon.png"
  },
  "permissions": ["tabs", "activeTab", "http://*/", "https://*/"],
  "commands": {
    "reverseSearch": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "activate reverse image search"
    }
  }
}
